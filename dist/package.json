{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "type": "commonjs",
  "main": "server.ts",
  "scripts": {
    "start": "node src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "test": "db-migrate --env test up && jasmine-ts && db-migrate db:drop test",
    "tsc": "tsc",
    "mig-up": "db-migrate up -m ./database/migrations   --config ./database.json",
    "mig-down": "db-migrate reset -m ./database/migrations  --config ./database.json"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "pg-connection-string": "^2.5.0",
    "pg-cursor": "^2.7.3",
    "pg-pool": "^3.5.1",
    "pg-query-stream": "^4.2.3",
    "supertest": "^6.2.3",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@ert78gb/jasmine-ts": "^0.3.2",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^4.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "jasmine": "^4.1.0",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  }
}
